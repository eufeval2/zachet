    "def Fib(n):\n",
    "    if n <= 0:\n",
    "        return  0\n",
    "    else:\n",
    "        if n == 1:\n",
    "            return 1\n",
    "        else:\n",
    "            return Fib(n-1)+ Fib(n-2)"
   ]
